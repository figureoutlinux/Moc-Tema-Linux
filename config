# Basta colocar esse arquivo dentro da pasta .moc que fica localizada dentro da pasta home de seu SO
# Fique a vontade em alterar as configurações para puro engrandecimento de seu conhecimento é válido
# Config editada, se ficou ruim basta procurar a config.exemple no caminho onde se encontra o vídeo no youtube

FormatString = "%(n:%n :)%(a:%a - :)%(t:%t:)%(A: \(%A\):)"

OSSDevice = /dev/dsp
OSSMixerDevice = /dev/mixer
OSSMixerChannel1 = pcm             # 'pcm', 'master' or 'speaker'
OSSMixerChannel2 = master          # 'pcm', 'master' or 'speaker'

ALSADevice = default
ALSAMixer1 = PCM
ALSAMixer2 = Master

Softmixer_SaveState = yes

Equalizer_SaveState = yes

ShowFormat = yes

# para usar o tema que personalizei basta mudar a linha / nome do theme conforme o vídeo do canal
XTermTheme = figureoutlinux_theme
